profiles {

    quay {
        params {
            //container = "quay.io/nextflow/bash:latest"
            container = "nextflow/bash:latest"
        }

        process.container = "${params.container}"
        docker.registry = "quay.io"
    }

    dockerhub {
        params {
            // container = "gwright99/private_nextflow_bash:latest"
            container = "gwright99/private_nextflow_bash:latest"
        }

        process.container = "${params.container}"
        docker.registry = "docker.io"
    }
}